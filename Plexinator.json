() {\n    isOpen = true;\n    $('.hamburger-menu_1648452437530_395').addClass('open');\n    $('.hamburger-menu_1648452437530_395').removeClass('closed');\n  }\n  function toggle() {\n    if(isOpen) close();\n    else open();\n  }\n  var silexElement = $('.hamburger-menu_1648452437530_395').closest('.editable-style');\n  function move(data) {\n    var offset = silexElement.offset();\n    $('.hamburger-menu_1648452437530_395 .nav-container').css({\n      'transform': 'translate(' + (data.scrollLeft - (offset.left/data.scale)) + 'px, ' + (data.scrollTop - (offset.top/data.scale)) + 'px)'\n    });\n  }\n  $('.hamburger-menu_1648452437530_395 .hamburger-btn').click(function() { toggle(); });\n  $(document).on('silex.scroll silex.resize', function(e) {\n    close();\n    move(window.silex);\n  });\n  close();\n})();\n</script>\n"},{"id":"silex-id-1648446517194-10","tagName":"SECTION","type":"section-element","alt":null,"title":null,"isSectionContent":false,"visibility":{"desktop":true,"mobile":true},"style":{"mobile":{},"desktop":{"position":"static"}},"data":{"component":null},"children":["silex-id-1648446517194-11"],"pageNames":[],"classList":[],"attr":{},"link":null,"enableEdit":false,"enableDrag":true,"enableDrop":true,"enableResize":{"top":false,"bottom":false,"left":false,"right":false},"selected":false,"useMinHeight":true,"innerHtml":""},{"id":"silex-id-1648446517194-11","tagName":"DIV","type":"container-element","alt":null,"title":null,"isSectionContent":true,"visibility":{"desktop":true,"mobile":true},"style":{"desktop":{"height":"20px","background-color":"rgba(30,153,174,1)","position":"relative","margin-left":"auto","margin-right":"auto","display":"block","flex-wrap":"nowrap","align-items":"stretch","justify-content":"stretch","border-radius":"100px 100px 0 0 ","background-image":"","background-position":"center center","background-attachment":"fixed","background-repeat":"no-repeat","background-size":"cover"},"mobile":{}},"data":{"component":null},"children":["